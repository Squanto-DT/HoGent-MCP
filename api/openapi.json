{
    "openapi": "3.0.0",
    "info": {
        "title": "Student Info API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000"
        }
    ],
    "paths": {
        "/api/restaurant/{name}": {
            "get": {
                "summary": "Get restaurant information by name",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response"
                       
                    }
                }
            }
        },
        "/api/campus/{facility}": {
            "get": {
                "summary": "Get faculty information",
                "parameters": [
                    {
                        "name": "facility",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response"                        
                    }
                }
            }
        }
    }
}
 